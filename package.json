  "scripts": {
    "dev": "tsnd -r tsconfig-paths/register --transpile-only --ignore-watch node_modules --respawn --poll src/shared/infra/http/server.ts",
    "dev:debug": "tsnd -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "migration:run": "tsnd -r tsconfig-paths/register node_modules/typeorm/cli migration:run",
    "migration:create": "tsnd -r tsconfig-paths/register node_modules/typeorm/cli migration:create -n",
    "migration:revert": "tsnd -r tsconfig-paths/register node_modules/typeorm/cli migration:revert",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest",
    "seed:admin": "tsnd -r tsconfig-paths/register src/shared/infra/database/typeorm/seed/admin.ts"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.35",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^27.0.6",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "csv-parse": "^4.15.3",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "tsyringe": "^4.5.0",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
    "yamljs": "^0.3.0"
  }
}
