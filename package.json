{
  "name": "rentx",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "dev": "tsnd -r tsconfig-paths/register --transpile-only --ignore-watch node_modules --respawn --poll src/shared/infra/http/server.ts",
    "dev:debug": "tsnd -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "migration:run": "tsnd -r tsconfig-paths/register node_modules/typeorm/cli migration:run",
    "migration:create": "tsnd -r tsconfig-paths/register node_modules/typeorm/cli migration:create -n",
    "migration:revert": "tsnd -r tsconfig-paths/register node_modules/typeorm/cli migration:revert",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "SET NODE_ENV=test&&jest --detectOpenHandles --runInBand",
    "seed:admin": "tsnd -r tsconfig-paths/register src/shared/infra/database/typeorm/seed/admin.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mime": "^2.0.3",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.35",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^27.0.6",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "aws-sdk": "^2.975.0",
    "bcryptjs": "^2.4.3",
    "csv-parse": "^4.15.3",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.5.2",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.3",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.4",
    "swagger-ui-express": "^4.1.6",
    "tsyringe": "^4.5.0",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
    "yamljs": "^0.3.0"
  }
}
